/ Redirect to login\n    window.location.href = \"./login\";\n  }\n}\n\n\nfunction App() {\n  return (\n    <Provider store={store}>\n          <div className=\"App\">\n          <Router>\n              <Switch>\n                <Route exact path=\"/\" component={Homepage} />\n                <Route path=\"/customer/access/:action\" exact component={CustomerAccessPage} />\n                <PrivateRoute path=\"/profile\" exact component={Profile} />\n                <Route path=\"/search_instructor\" exact component={SearchInstructor} />\n                <PrivateRoute path=\"/rate_instructor\" exact component={RateInstructor} />\n                <PrivateRoute path=\"/users_list\" exact component={UsersList} />\n                {/* <Route exact path=\"/register\" component={Register} />\n                <Route exact path=\"/login\" component={Login} /> */}\n              </Switch>\n            </Router>\n          </div>\n      </Provider>\n  );\n}\n\nexport default App;\n","/Users/alfredoneto/Documents/Last Semester/drive-buddy-project/client/src/store.js",[],"/Users/alfredoneto/Documents/Last Semester/drive-buddy-project/client/src/actions/authActions.js",[],"/Users/alfredoneto/Documents/Last Semester/drive-buddy-project/client/src/utils/setAuthToken.js",[],"/Users/alfredoneto/Documents/Last Semester/drive-buddy-project/client/src/components/private-route/PrivateRoute.js",[],"/Users/alfredoneto/Documents/Last Semester/drive-buddy-project/client/src/components/dashboard/Dashboard.js",[],"/Users/alfredoneto/Documents/Last Semester/drive-buddy-project/client/src/containers/RateInstructor/index.js",[],"/Users/alfredoneto/Documents/Last Semester/drive-buddy-project/client/src/containers/UsersList/index.js",[],"/Users/alfredoneto/Documents/Last Semester/drive-buddy-project/client/src/containers/CustomerAccessPage/index.js",[],"/Users/alfredoneto/Documents/Last Semester/drive-buddy-project/client/src/containers/SearchInstructor/index.js",[],"/Users/alfredoneto/Documents/La